{
	"manifest_version": 2,
	"name": "尔雅Crack",
	"version": "1.0.0",
	"description": "by omsfuk",
	"icons": {
		"16": "images/icon.png",
		"48": "images/icon.png",
		"128": "images/icon.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "images/icon.png",
			"38": "images/icon.png"
		},
		"default_title": "尔雅Crack",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["*://mooc1-2.chaoxing.com/*"],
			"js": ["js/crack.js"],
			"all_frames": true
		}
	]
}